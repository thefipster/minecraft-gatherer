  </ItemGroup>

  <ItemGroup>
    <Folder Include="wwwroot\img\mobs\" />
    <Folder Include="wwwroot\img\players\" />
  </ItemGroup>

  <ItemGroup>
    <None Include="wwwroot\img\logo.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>



